# Visto che quello schifo di VScode non lo include di default come su altri editor
# https://marketplace.visualstudio.com/items?itemName=EditorConfig.EditorConfig

root = true
[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8

indent_style = space
indent_size = 3
tab_width = 3

max_line_length = 90
trim_trailing_whitespace = true
